<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/feedFragment"
    android:id="@+id/nav_graph">
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.example.yadshniya.Fragments.feedFragment"
        android:label="feed"
        tools:layout="@layout/activity_feed">
        <action
            android:id="@+id/action_to_feed_Tab"
            app:destination="@id/feedFragment" />
    </fragment>
    <fragment
        android:id="@+id/newPostFragment"
        android:name="com.example.yadshniya.Fragments.newPostFragment"
        android:label="new_post"
        tools:layout="@layout/activity_new_post">
        <action
            android:id="@+id/action_to_new_post_Tab"
            app:destination="@id/newPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.yadshniya.Fragments.profileFragment"
        android:label="profile"
        tools:layout="@layout/activity_profile">
        <action
            android:id="@+id/action_to_profile_Tab"
            app:destination="@id/profileFragment" />
    </fragment>
</navigation>